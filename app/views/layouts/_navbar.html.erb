<div class="navbar navbar-fixed-top" style="opacity:0.8">
  <div class="navbar-inner">
    <div class="container">
      <%= link_to "CollegeStrong", root_url, class: "brand" %>
      <div class="btn-group pull-left">
        <ul class="nav">
          <li><%= link_to "How it Works" %></li>
          <li><%= link_to "Workouts", workouts_path %></li>
        </ul>
      </div>
      <div class="btn-group pull-right">
        <% if user_signed_in? %>
        <%= render partial: "/layouts/navbar_signed_in" %>
        <% else %>
        <%= render partial: "/layouts/navbar_signed_out" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
