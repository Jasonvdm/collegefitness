<%= simple_form_for(@workout) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
	<%= f.input :avatar %>
    <%= f.input :equipment %>
    <%= f.input :equipmentneeded %>
    <%= f.input :description %>
    <%= f.input :difficulty %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
